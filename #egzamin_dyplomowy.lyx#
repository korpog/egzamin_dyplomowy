\begin_layout Plain Layout


\backslash
section{Warunek konieczny i dostateczny istnienia ekstremum lokalnego funkcji wielu zmiennych.
 Przykłady}
\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout


\backslash
subsection{Warunek konieczny}
\end_layout

\begin_layout Plain Layout

Jeśli funkcja $f:
 
\backslash
mathbb{R}^n 
\backslash
to 
\backslash
mathbb{R}$ ma ekstremum lokalne w punkcie $x_0$,
 to wszystkie pochodne cząstkowe w tym punkcie są równe zero:
\end_layout

\begin_layout Plain Layout


\backslash
[
\end_layout

\begin_layout Plain Layout


\backslash
frac{
\backslash
partial f}{
\backslash
partial x_i}(x_0) = 0 
\backslash
quad 
\backslash
text{dla } i = 1,
 
\backslash
ldots,
 n
\end_layout

\begin_layout Plain Layout


\backslash
]
\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout


\backslash
subsection{Warunek dostateczny}
\end_layout

\begin_layout Plain Layout

Niech $f:
 
\backslash
mathbb{R}^n 
\backslash
to 
\backslash
mathbb{R}$ będzie funkcją klasy $C^2$ w otoczeniu punktu $x_0$.
 Jeśli:
\end_layout

\begin_layout Plain Layout


\backslash
begin{itemize}
\end_layout

\begin_layout Plain Layout

    
\backslash
item $
\backslash
nabla f(x_0) = 0$
\end_layout

\begin_layout Plain Layout

    
\backslash
item Macierz Hessego $H(x_0)$ jest:
\end_layout

\begin_layout Plain Layout

    
\backslash
begin{itemize}
\end_layout

\begin_layout Plain Layout

        
\backslash
item dodatnio określona $
\backslash
Rightarrow$ minimum lokalne
\end_layout

\begin_layout Plain Layout

        
\backslash
item ujemnie określona $
\backslash
Rightarrow$ maksimum lokalne
\end_layout

\begin_layout Plain Layout

    
\backslash
end{itemize}
\end_layout

\begin_layout Plain Layout


\backslash
end{itemize}
\end_layout

\begin_layout Plain Layout

to $f$ ma ekstremum lokalne w $x_0$.
\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout


\backslash
subsection{Przykłady}
\end_layout

\begin_layout Plain Layout


\backslash
begin{itemize}
\end_layout

\begin_layout Plain Layout

    
\backslash
item $f(x,y) = x^2 + y^2$ ma minimum w $(0,0)$
\end_layout

\begin_layout Plain Layout

    
\backslash
item $f(x,y) = x^2 - y^2$ ma punkt siodłowy w $(0,0)$
\end_layout

\begin_layout Plain Layout

    
\backslash
item $f(x,y) = x^3 + y^3 - 3xy$ ma minimum lokalne w $(1,1)$ i maksimum lokalne w $(-1,-1)$
\end_layout

\begin_layout Plain Layout


\backslash
end{itemize}
\end_layout

